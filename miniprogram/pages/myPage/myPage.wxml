    <view class="item live" bindtap="">
      <view class="image">
        <image src="" mode="widthFix" />
      </view>
      <text>发布直播</text>
    </view>
    <view class="item message" bindtap="">
      <view class="image">
        <image src="" mode="widthFix" />
      </view>
      <text>物资信息</text>
    </view>
  </view>
  <view>
    <view bindtap="">我的收藏</view>
